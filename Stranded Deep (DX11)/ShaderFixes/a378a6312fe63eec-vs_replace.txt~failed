// hlsltool.py -I ../.. --auto-fix-vertex-halo --fix-unity-reflection --fix-unity-frustum-world --only-autofixed --fxc /home/dss/fxc.exe a378a6312fe63eec-vs_replace.txt
if (r0.w != 1.0) { r0.x += separation * (r0.w - convergence); }

  r3.xz = float2(0.5,0.5) * r0.xw;
  r1.w = cb1[5].x * r0.y;
  r3.w = 0.5 * r1.w;
  o8.xy = r3.xw + r3.zz;
  o6.xy = v3.xy * cb3[0].xy + cb3[0].zw;

  // o0.xyzw = r0.xyzw; // Relocated from here with hlsltool.py

